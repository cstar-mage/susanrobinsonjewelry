      jQuery('#search_result_header_table').css('display', 'none');

      jQuery('#diamonds_recently_viewed_table').css('display','block');

      jQuery('#diamonds_comparison_table').css('display','none');

      jQuery('#diamonds_search_table').css('display','none');

    });

    jQuery('#tab_diamonds_comparison').click(function(){

      jQuery('.search-nothing-tips').addClass('hidden');

      jQuery('#listContainer').removeClass('hidden');

      jQuery('.scrollbar').css('visibility', 'hidden');

      jQuery('#cmp-scrollbar').css('visibility','visible');

