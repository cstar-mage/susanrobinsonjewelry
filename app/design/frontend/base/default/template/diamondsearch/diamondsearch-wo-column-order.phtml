      jQuery('#search_result_header_table').css('display', 'none');

      jQuery('#diamonds_recently_viewed_table').css('display','none');

      jQuery('#diamonds_comparison_table').css('display','block');

      jQuery('#diamonds_search_table').css('display','none');

